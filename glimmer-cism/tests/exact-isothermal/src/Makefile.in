	$(PPFCCOMPILE) -c -o $@ $(FCFLAGS_f90) $<
.f.o:
	$(PPFCCOMPILE) -c -o $@ $(FCFLAGS_f) $<
.F90.o:
	$(PPFCCOMPILE) -c -o $@ $(FCFLAGS_F90) $<

